# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /                        controllers.courseRegistration.index()
GET         /courses/overview        controllers.courseRegistration.showOverview()
GET         /courses/register        controllers.courseRegistration.showCourseRegistration()
POST        /courses/register        controllers.courseRegistration.registerCourses()

# Test page
GET         /application             controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file            controllers.Assets.at(path="/public", file)
