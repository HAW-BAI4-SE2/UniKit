<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="CourseModel" table="`haw_se2_testdata`.`COURSE`">
      <meta attribute="class-description">
         This class contains the course detail. 
      </meta>
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="name" column="name" type="string" not-null="true"/>
      <property name="shortcut" column="shortcut" type="string" not-null="true"/>
      <property name="minTeamSize" column="min_team_size" type="int" not-null="true"/>
      <property name="maxTeamSize" column="max_team_size" type="int" not-null="true"/>
   </class>
   <set name="student" table="`haw_se2_testdata`.`COMPLETED_COURSE`" lazy="true">
      <key column="courseNumber"/>
      <many-to-many class="StudentModell" column="studentNumber"/>
   </set>
   <set name="student" table="`haw_se2_testdata`.`COURSE_REGISTRATION`" lazy="true">
      <key column="courseNumber"/>
      <many-to-many class="StudentModell" column="studentNumber"/>
   </set>
</hibernate-mapping>