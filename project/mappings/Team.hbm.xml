<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="TeamModel" table="`haw_se2`.`TEAM`">
      <meta attribute="class-description">
         This class contains the team detail. 
      </meta>
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="courseId" column="course_id" type="int" not-null="true"/>
   </class>
    <set name="student" table="`haw_se2`.`STUDENT_TO_TEAM`" lazy="true">
      <key column="teamID"/>
      <many-to-many class="StudentModel" column="student_number"/>
    </set>
</hibernate-mapping>